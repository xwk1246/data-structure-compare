{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc 建置使用中檔案",
            "command": "/usr/bin/gcc",
            "args": [
                "-g",
                "${file}",
                "./array.c",
                "./bst.c",
                "datagenerator.c",
                "hash.c",
                "linkedlist.c",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "偵錯工具產生的工作。"
        }
    ],
    "version": "2.0.0"
}